<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.liuzhuoming23.svea.app.mapper.LogDetailMapper">

  <insert id="insert" parameterType="com.github.liuzhuoming23.svea.app.domain.LogDetail">
    insert into log_detail
    (`level`,method,args,return_val,account_id,username,description,`exception`,run_time)
    values
    (#{level},#{method},#{args},#{returnVal},#{accountId},#{username},#{description},#{exception},#{runTime})
  </insert>
</mapper>
